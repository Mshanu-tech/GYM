<style>
  table {
    border-collapse: collapse;
    width: 100%;
    color: #333;
    font-family: Arial, sans-serif;
    font-size: 16px;
  }
  th, td {
    padding: 10px;
    text-align: left;
    vertical-align: middle;
  }
  th {
    background-color: #008CBA;
    color: white;
  }
  tr:nth-child(even) {
    background-color: #f2f2f2;
  }
  tr:hover {
    background-color: #ddd;
  }
</style>

<table>
  <tr>
    <th>Date</th>
    <th>isPresent</th>
  </tr>
  <%arr.forEach(el =>{%>
  <tr>
    <td><%=el.date %></td>
    <td><%=el.present %></td>
  </tr>
  <% }) %>
</table>
